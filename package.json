{
  "name": "autoexif",
  "version": "1.0.4",
  "description": "CLI tool for removing excess EXIF data from images",
  "main": "dist/index.js",
  "bin": {
    "autoexif": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsup src/index.ts --watch",
    "build": "tsup src/index.ts --format cjs --dts",
    "start": "node dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "exif",
    "data",
    "simple",
    "image",
    "photo",
    "metadata",
    "exiftool",
    "cli",
    "remove",
    "auto",
    "automatic"
  ],
  "author": "Cyprian Zdebski <shipurjan@gmail.com>",
  "homepage": "https://github.com/shipurjan/autoexif#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shipurjan/autoexif.git"
  },
  "bugs": {
    "url": "https://github.com/shipurjan/autoexif/issues",
    "email": "shipurjan@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "22.13.5",
    "prettier": "3.5.2",
    "tsup": "8.4.0",
    "typescript": "5.7.3"
  },
  "dependencies": {
    "commander": "13.1.0",
    "exiftool-vendored": "29.1.0"
  }
}
